        get_timeformat:function(t){
            let time_format = "";
            let time;
            let format = t.split("T");
            let day = format[0].split("-");
            time = format[1].split(".")[0].split(":");
            time_format += day[1] + "-" + day[2] + " " + time[0] + ":" + time[1] + ":" + time[2];
            return time_format;
        },
