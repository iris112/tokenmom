        draw_spread: function (best_asks,best_bids){
            let asks;
            let bids;
            if(best_asks == null){
                asks = 0;
            }else{
                asks = best_asks;
            }
            if(best_bids == null){
                bids = 0;
            }else{
                bids = best_bids;
            }
            let spread = (asks - bids).toFixed(8);
            //Set values
            $(".spread").attr("best_asks",asks);
            $(".spread").attr("best_bids",bids);
            $(".spread").find(":nth-child(2)").children("strong").text(spread);
        },
